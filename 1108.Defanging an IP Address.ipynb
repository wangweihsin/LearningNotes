    "class Solution:\n",
    "    def defangIPaddr(self, address: str) -> str:\n",
    "        b=''\n",
    "        for i in address:\n",
    "            if i=='.':\n",
    "                b+='[.]'\n",
    "            else:\n",
    "                b+=i\n",
    "        return b"
